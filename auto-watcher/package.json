{
  "name": "ones-app-example",
  "private": true,
  "version": "1.0.0",
  "description": "ONES App NestJS TypeScript example with React frontend",
  "license": "MIT",
  "keywords": [
    "ones",
    "nestjs",
    "react",
    "typescript",
    "example",
    "starter"
  ],
  "scripts": {
    "build": "npm run build:web && nest build",
    "build:web": "cd web && npm run build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "dev": "npm run build:web && nest start --debug --watch",
    "prod": "node dist/main",
    "predev:nest": "npm --prefix web run build",
    "lint": "eslint \"{src,web}/**/*.ts\" --fix",
    "init": "cd web && npm install && cd .. && npm install && npm run build && cp env.example .env && npm run gen:app-id",
    "gen:app-id": "node ./scripts/generate-app-id.mjs",
    "validate-manifest": "npx op2 validate -m ./manifest.json -s /Users/malvin/Coding/appbuilder/ones-specs/app_manifest_schema.json"
  },
  "engines": {
    "npm": ">=10.0.0",
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.17",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/serve-static": "^5.0.4",
    "@nestjs/typeorm": "^11.0.0",
    "jsonwebtoken": "^9.0.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "sqlite3": "^5.1.7",
    "typeorm": "^0.3.27"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.8",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^22.10.7",
    "@types/supertest": "^6.0.2",
    "concurrently": "^9.2.1",
    "dotenv": "^17.2.3",
    "ejs": "^3.1.10",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "globals": "^15.14.0",
    "inquirer": "^9.3.8",
    "kolorist": "^1.8.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@nestjs/core",
      "@swc/core"
    ]
  }
}